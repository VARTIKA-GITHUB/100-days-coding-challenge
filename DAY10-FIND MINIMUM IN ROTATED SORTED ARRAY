DAY10-FIND MINIMUM IN ROTATED SORTED ARRAY
class Solution {
public:
    int findMin(vector<int>& nums) {
        int n=nums.size();
        int low=0;
        int high=n-1;
        if (nums[low] <= nums[high]) {
            return nums[low];
        }
        while(low<high)
        {
            int mid=(low+high)/2;
            if(nums[mid]>nums[high])
            {
                // that is minimum lies in the right sorted block 
                low=mid+1; 
            }
            else{
               high=mid;

            }
        }
      return nums[low];  
    }
};
